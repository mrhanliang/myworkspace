package cn.cqupt.work1;

public class Test3work5 {
	public static void main(String args[]){
		long i = 1;
		long j = 1;
		long sum1 = 0;
		long sum2 = 0;
		while(i <= 100){
			sum1 = sum1 + j;
			i++;
			j = j*i;
		}
		System.out.println(sum1);
		while(i <= 200){
			sum2 = sum2 + j;
			i++;
			j = j * i;
		}
	}
}
