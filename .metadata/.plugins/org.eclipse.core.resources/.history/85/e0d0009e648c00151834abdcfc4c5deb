package cn.cqupt.work1;

public class Test3_2 {
	public static void main(String args[]){
		char a[] = {'金','木','水','火','土'};
		char s = 'a';
		for(int i = 0 ; i < a.length ; i++){
			a[i] = (char)(a[i]^s);
		}
		System.out.println("加密：");
		for(int i = 0 ; i < a.length ; i++){
			System.out.println(a[i]);
		}
		for(int i = 0 ; i < a.length ; i++){
			a[i] = (char)(a[i]^s);
		}
		System.out.println("源码：");
		for(int i = 0 ; i < a.length ; i++){
			System.out.println(a[i]);
		}
	}
}
