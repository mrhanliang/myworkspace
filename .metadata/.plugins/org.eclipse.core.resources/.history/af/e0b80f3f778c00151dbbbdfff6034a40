package cn.cqupt.work1;

public class Test3work4 {
	public static void main(String args[]){
		int i = 1;
		double j;
		double sum = 0;
		while(i <= 3){
			if(i % 2 == 0){
				j = -1*(1 /(2 * i - 1));
				System.out.println(j);
			}
			else{
				j = 1/(2 * i -1);
				System.out.println(j);
			}
			//sum = sum + j;
			i++;
		}
		System.out.printf("1-1/3+1/5-1/7+....前10000项之和为：%100000000.1000000000f%n" , sum);
	}
}
