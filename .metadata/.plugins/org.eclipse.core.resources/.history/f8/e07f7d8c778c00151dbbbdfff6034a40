package cn.cqupt.work1;

public class Test3work4 {
	public static void main(String args[]){
		int i = 1;
		double j;
		double sum = 0;
		while(i <= 1){
			j = ((-1)^(i+1));
					System.out.println(j);
			sum = sum + j;
			System.out.println(sum);
			i++;
		}
		System.out.printf("1-1/3+1/5-1/7+....前10000项之和为：%10000.10000f" , sum);
	}
}
